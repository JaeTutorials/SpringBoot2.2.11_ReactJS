<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="KimJeongKyun.toyShop.Router.MyBatisDao">

    <select id="selectUser" parameterType="MyBatisModel"  resultType="MyBatisModel">
        /* selectUser */ /* SQL id => 로그 검색, jdbc.resultsettable => 결과 data 검색 */
        SELECT *
        FROM TB_BOARD
    </select>

    <select id="selectUser1" parameterType="int"  resultType="MyBatisModel">
        /* selectUser1 */
        SELECT *
        FROM TB_BOARD
        WHERE IDX = ${idx}
    </select>

</mapper>
